public class CriaConta {
	public static void main(String[] args) {
		Conta primeiraConta = new Conta();
		primeiraConta.saldo = 200;
		System.out.println(primeiraConta.saldo);
		
		primeiraConta.saldo += 100;
		System.out.println(primeiraConta.saldo);
		
		Conta segundaConta = new Conta();
		segundaConta.saldo = 50;
		
		System.out.println("Na primeira conta tem: " + primeiraConta.saldo);
		System.out.println("Na segunda conta tem: " + segundaConta.saldo);
		
		System.out.println(primeiraConta.agencia);
		System.out.print(primeiraConta.titular);
		
		
		System.out.println(segundaConta.agencia);
		System.out.print(segundaConta.titular);
		
		segundaConta.agencia = 162;
		System.out.print(segundaConta.agencia);
		
		
		if (primeiraConta == segundaConta) {
			System.out.println("Mesma conta");
		} else {
			System.out.println("Contas diferentes");
		}
		
		
		
		
		
	}
	
}
